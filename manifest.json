{
  "manifest_version": 3,
  "name": "Chrome Proxy Manager",
  "version": "0.2.0",
  "description": "A Chrome extension for managing proxy rules",
  "permissions": [
    "storage",
    "proxy",
    "tabs"
  ],
  "host_permissions": [
    "*://*/*"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "action": {
  },
  "icons": {
    "16": "icon/icon16.png",
    "32": "icon/icon32.png",
    "48": "icon/icon48.png",
    "128": "icon/icon128.png",
    "256": "icon/icon256.png",
    "512": "icon/icon512.png"
  }
}
